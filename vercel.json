{
    "version": 2,
    "env": {
        "NPM_INSTALL": "npm install --legacy-peer-deps"
    },
    "builds": [
        {
        "src": "src/main.ts",
        "use": "@vercel/node"
        },
        {
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}