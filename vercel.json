{
    "version": 2,
    "builds": [
        {
        "src": "src/main.ts",
        "use": "@vercel/node"
        },
        {
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ],
    "build": {
        "env": {
        "NPM_CONFIG_PRODUCTION": "false"
        },
        "command": "npm install --legacy-peer-deps && npm run build"
    }
}