{
    "version": 2,
    "builds": [
    {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "output": {
        "clean": true
        }
    },
    {
        "use": "@vercel/static",
        "output": {
        "clean": true
        }
    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
    }
    ],
    "env": {
    "NPM_INSTALL": "npm install --legacy-peer-deps"
    },
    "hooks": {
    "postbuild": "npm run vercel-start"
    }
}