{
    "version": 2,
    "builds": [
        {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "build": {
            "env": {
            "NPM_INSTALL": "npm install --legacy-peer-deps"
            },
            "command": "npm run build"
        },
        "dev": {
            "command": "npm run start:dev"
        },
        "routes": [
            {
            "src": "/(.*)",
            "dest": "/src/main.ts"
            }
        ]
        }
    ]
}